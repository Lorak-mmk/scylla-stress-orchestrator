#!/bin/python3


import sys
import os
sys.path.insert(1, f"{os.environ['SSO']}/src/")

import yaml
from sso import terraform

if len(sys.argv) == 1:
    print("No data directory")
    exit(1)
else:
    data_dir = sys.argv[1]

scylla_monitoring_path="/eng/scylla/scylla-monitoring-scylla-monitoring-3.7.0"

cmd = f"{scylla_monitoring_path}/start-all.sh -d {data_dir}"
os.system(cmd)
     
